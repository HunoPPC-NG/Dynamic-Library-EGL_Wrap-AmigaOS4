copy SDK/lib/libegl_wrap.so to SDK:Local/newlib/lib/
copy SDK/lib/libSDLegl_wrap.a to SDK:Local/newlib/lib/
copy SDK/lib/pkgconfig/ to SDK:Local/newlib/lib/pkgconfig/
copy SDK/bin/ to SDK:Local/newlib/bin/
mkdir SDK:Local/newlib/include/EGL
mkdir SDK:Local/newlib/include/EGLSDL
mkdir SDK:Local/newlib/include/EGLSDL2
